{
  "include": [
    "src/"
  ],
  "exclude": [
    "node_modules"
  ],
    "compilerOptions": {
      "target": "ES2019",
      "module": "CommonJS",
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "esModuleInterop": true
  }
}
