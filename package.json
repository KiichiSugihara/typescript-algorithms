{
  "name": "typescript-algorithms",
  "version": "1.0.0",
  "description": "Algorithms &amp; Data Structure with TypeScript",
  "main": "index.js",
  "scripts": {
    "compile": "yarn ts-node src/*",
    "test": "jest",
    "lint": "eslint \"**/*.ts\"",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KiichiSugihara/typescript-algorithms.git"
  },
  "author": "Kiichi Sugihara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KiichiSugihara/typescript-algorithms/issues"
  },
  "homepage": "https://github.com/KiichiSugihara/typescript-algorithms#readme",
  "dependencies": {
    "@types/node": "^14.0.14",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "lint-staged": "^10.2.11"
  },
  "jest": {
    "preset": "ts-jest",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  }
}
